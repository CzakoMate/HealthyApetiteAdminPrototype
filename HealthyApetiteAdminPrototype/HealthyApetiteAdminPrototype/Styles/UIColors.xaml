<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="StandardButtonBackground" Color="#095E7D"></SolidColorBrush>
    <SolidColorBrush x:Key="StandardButtonEnabled" Color="#01CB11"></SolidColorBrush>
    <SolidColorBrush x:Key="StandardButtonNotEnabled" Color="Red"></SolidColorBrush>
    <SolidColorBrush x:Key="StandardButtonFocused" Color="#EEEEFF"></SolidColorBrush>
        <!--Background Colors-->
    <SolidColorBrush x:Key="backgroundPrimaryColor" Color="#f4f4f4"/>
    <!--Window Border Colors-->
    <Color x:Key="borderColor1">#f4f4f4</Color>
    <Color x:Key="borderColor2">White</Color>
    <Color x:Key="borderColor3">#2e313e</Color>
    <Color x:Key="borderColor4">#363944</Color>
